{
  "name": "production-project",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npx webpack serve --env port=5001",
    "start:server": "node ./json-server/index.js",
    "build:prod": "npx webpack --env mode=production",
    "build:dev": "npx webpack --env mode=development",
    "lint": "npx eslint \"**/*.{ts,tsx}\"",
    "lint:fix": "npx eslint \"**/*.{ts,tsx}\" --fix",
    "lint-scss": "npx stylelint \"**/*.scss\"",
    "lint-scss:fix": "npx stylelint \"**/*.scss\" --fix",
    "unit": "jest --config ./config/jest/jest.config.ts",
    "loki": "npx loki test",
    "loki:ok": "npx loki approve",
    "loki:ci": "npx loki --requireReference --reactUri file:./storybook-static",
    "storybook": "start-storybook -p 6006 -c ./config/storybook",
    "build-storybook": "build-storybook -c ./config/storybook",
    "test:ui:json": "node scripts/generate-visual-json-report.js",
    "test:ui:html": "npx reg-cli --from .loki/report.json --report .loki/report.html",
    "test:report": "npm run test:ui:json && npm run test:ui:html",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
